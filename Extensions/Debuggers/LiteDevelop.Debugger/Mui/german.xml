      <Item Id="Stopped">Gestoppt</Item>
      <Item Id="WaitSleepJoin">Wartend</Item>
      <Item Id="Suspended">Angehalten</Item>
      <Item Id="AbortRequested">Abbrechen angefordert</Item>
      <Item Id="Aborted">Abgebrochen</Item>
    </ItemGroup>
  </ItemGroup>

  <ItemGroup Id="CallStack">
    <Item Id="Title">Aufrufliste</Item>
    <ItemGroup Id="Messages">
      <Item Id="ExternalCode">[externer Code]</Item>
