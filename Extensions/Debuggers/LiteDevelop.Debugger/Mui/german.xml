      <Item Id="Offset">Offset</Item>
    </ItemGroup>
  </ItemGroup>

  <ItemGroup Id="ExceptionDialog">
    <Item Id="Title">Ausnahme</Item>
    <Item Id="ExceptionOccured">Eine Ausnahme ist aufgetreten</Item>
    <Item Id="File">Datei:</Item>
